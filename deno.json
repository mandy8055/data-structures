{
  "name": "@msk/data-structures",
  "version": "0.1.0",
  "exports": "./src/mod.ts",
  "license": "MIT",
  "publish": {
    "include": ["src"],
    "exclude": ["src/tests", ".github", ".vscode"]
  },
  "tasks": {
    "test": "deno test --allow-none",
    "check": "deno check **/*.ts",
    "lint": "deno lint",
    "fmt": "deno fmt"
  },
  "fmt": {
    "include": ["src/", "tests/"],
    "lineWidth": 80,
    "indentWidth": 2,
    "singleQuote": true
  },
  "lint": {
    "include": ["src/", "tests/"],
    "rules": {
      "tags": ["recommended"]
    }
  },
  "compilerOptions": {
    "strict": true
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.6"
  }
}
